{
    "mcpServers": {
        "o3": {
            "command": "npx",
            "args": [
                "o3-search-mcp"
            ],
            "env": {
                "SEARCH_CONTEXT_SIZE": "medium",
                "REASONING_EFFORT": "medium"
            }
        },
        "github": {
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "-e",
                "GITHUB_PERSONAL_ACCESS_TOKEN",
                "ghcr.io/github/github-mcp-server"
            ],
            "env": {
                "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
            }
        }
    }
}
